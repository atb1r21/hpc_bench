#!/bin/sh

ARCH=`uname -m`

case $ARCH in

    i?86)
	if   [ `/usr/bin/which pgf90 2>&1 | awk '{print NF}' - ` -eq 1 ]; then 
            echo i386_pgf90 
        elif [ `/usr/bin/which ifc   2>&1 | awk '{print NF}' - ` -eq 1 ]; then 
            echo i386_ifc 
        fi; 
	exit 0
	;;

    alpha)
	echo alpha
	exit 0
	;;
    
    IP??)
	echo sgi
	exit 0
	;;

    *)
	echo $ARCH
	exit 0
	;;

esac

exit 1
